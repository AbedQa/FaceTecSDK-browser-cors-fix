{
    "version": 2,
    "functions": {
        "api/orange-proxy.js": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/orange/(.*)",
            "destination": "/api/orange-proxy"
        },
        {
            "source": "/health",
            "destination": "/api/orange-proxy"
        }
    ]
}